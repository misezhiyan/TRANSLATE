	</bean>
	<bean id="sentense_srcDao" class="com.translate.dao.impl.Sentense_srcDaoImpl">
		<property name="queryDao" ref="queryDao" />
		<property name="updateDao" ref="updateDao" />
	</bean>
	<bean id="userDao" class="com.translate.dao.impl.UserDaoImpl">
		<property name="queryDao" ref="queryDao" />
		<property name="updateDao" ref="updateDao" />
	</bean>
	<bean id="word_resDao" class="com.translate.dao.impl.Word_resDaoImpl">
		<property name="queryDao" ref="queryDao" />
		<property name="updateDao" ref="updateDao" />
	</bean>
	<bean id="word_srcDao" class="com.translate.dao.impl.Word_srcDaoImpl">
		<property name="queryDao" ref="queryDao" />
		<property name="updateDao" ref="updateDao" />
	</bean>
</beans>
